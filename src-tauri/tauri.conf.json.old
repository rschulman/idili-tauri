{
  "package": {
    "productName": "idili-tauri",
    "version": "0.0.0"
  },
  "build": {
    "beforeDevCommand": "trunk serve",
    "devPath": "http://localhost:1420",
    "beforeBuildCommand": "trunk build",
    "distDir": "../dist"
  },
  "tauri": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "idili-tauri",
        "width": 800,
        "height": 600
      }
    ],
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
